{
  "name": "cesium-react-tailwind",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "npm run dev",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
